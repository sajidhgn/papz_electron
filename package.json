{
  "name": "papz_electron_frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "next-dev": "next dev",
    "next-build": "next build",
    "next-start": "next start",
    "electron-dev": "concurrently \"npm run dev\" \"npm run electron\"",
    "electron": "electron .",
    "electron-prod": "NODE_ENV=production electron .",
    "dev": "concurrently \"next\" \"electron .\"",
    "build": "next build && electron-builder",
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "package": "electron-packager . ElectronNextApp --platform=win32 --arch=x64 --out=dist --overwrite",
    "lint": "next lint"
  },
  "main": "electron/main.mjs",
  "build": {
    "appId": "com.example.kineticelectronapp",
    "productName": "KineticElectronApp",
    "directories": {
      "output": "dist"
    },
    "files": [
      "electron/**/*",
      "dist/**"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "dependencies": {
    "@plaiceholder/next": "^3.0.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-select": "^2.1.1",
    "axios": "^1.6.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "electron-is-dev": "^3.0.1",
    "gsap": "^3.12.5",
    "jquery": "^3.7.1",
    "lucide-react": "^0.314.0",
    "next": "14.2.3",
    "plaiceholder": "^3.0.0",
    "react": "^18",
    "react-dom": "^18",
    "react-layout-masonry": "^1.1.0",
    "react-toastify": "^10.0.4",
    "sweetalert2": "^11.6.13",
    "sweetalert2-react-content": "^5.0.7",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "electron": "^33.0.2",
    "electron-builder": "^25.1.8",
    "electron-packager": "^17.1.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1"
  }
}
